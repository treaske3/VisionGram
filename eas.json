{
  "cli": {
    "version": ">= 13.0.0"
  },
  "build": {
    "development": {
      "resourceClass": "m-medium",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "resourceClass": "m-medium",
      "distribution": "internal"
    },
    "production": {
      "resourceClass": "m-medium",
      "distribution": "app-store"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "${{ secrets.APP_STORE_APPLE_ID }}",
        "ascApiKeyPath": "./asc_api_key.json",
        "companyName": "VisionGram"
      }
    }
  }
}
